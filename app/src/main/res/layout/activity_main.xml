<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent"
    >


    <ImageButton
        android:id="@+id/entrega"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_below="@+id/cuenta"
        android:layout_toLeftOf="@+id/cuenta"
        android:layout_toStartOf="@+id/cuenta"
        android:background="@drawable/entrega" />
    <ImageButton
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/retiro"
        android:background="@drawable/moto"
        android:layout_alignTop="@+id/entrega"
        android:layout_toRightOf="@+id/cuenta"
        android:layout_toEndOf="@+id/cuenta"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp" />

    <ImageButton
        android:id="@+id/estadisticas"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignLeft="@+id/textnuevo"
        android:layout_alignStart="@+id/textnuevo"
        android:layout_below="@+id/textnuevo"
        android:background="@drawable/estadistica" />

    <ImageButton
        android:id="@+id/consulta"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignLeft="@+id/textentrega"
        android:layout_alignStart="@+id/textentrega"
        android:layout_below="@+id/textentrega"
        android:background="@drawable/consulta" />

    <ImageButton
        android:id="@+id/configuracion"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignLeft="@+id/textconsulta"
        android:layout_alignStart="@+id/textconsulta"
        android:layout_below="@+id/textconsulta"
        android:background="@drawable/configuracion" />

    <ImageButton
        android:id="@+id/nuevo"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignLeft="@+id/textretiro"
        android:layout_alignStart="@+id/textretiro"
        android:layout_below="@+id/textretiro"
        android:background="@drawable/nuevo" />

    <TextView
        android:id="@+id/name"
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:text="menu "
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:visibility="visible"
        tools:text="menu"
        android:layout_below="@+id/imageView"
        android:textStyle="bold"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/cuenta"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_below="@+id/name"
        android:layout_centerHorizontal="true"
        android:textStyle="bold"
        android:text="cuenta"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:visibility="visible"
        tools:text="cuenta" />

    <!-- saco boton por mientras-->
    <ImageButton
        android:id="@+id/btnLogout"
        style="@android:style/Widget.Holo.Light.Button.Small"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:text="@string/btn_logout"
        android:textAllCaps="false"
        android:background="@drawable/logout"

        android:visibility="visible"
        android:layout_below="@+id/textconfiguracion"
        android:layout_alignRight="@+id/imageView"
        android:layout_alignEnd="@+id/imageView"
        android:layout_marginTop="14dp" />
    <ImageView
        android:id="@+id/imageView"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/igps"
        android:layout_marginTop="16dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/textentrega"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:layout_alignLeft="@+id/entrega"
        android:layout_alignStart="@+id/entrega"
        android:layout_below="@+id/entrega"
        android:text="Entrega"
        android:textStyle="bold"
        android:textAlignment="center"
        android:visibility="visible"
        tools:text="Entrega" />

    <TextView
        android:id="@+id/textconfiguracion"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:text="Configuracion"
        android:textAlignment="center"
        android:visibility="visible"
        tools:text="Configuracion"
        android:textStyle="bold"
        android:layout_alignLeft="@+id/configuracion"
        android:layout_alignStart="@+id/configuracion"
        android:layout_below="@+id/configuracion" />

    <TextView
        android:id="@+id/textnuevo"
        android:layout_width="70dp"
        android:layout_height="20dp"
        android:text="Nuevo"
        android:textAlignment="center"
        android:visibility="visible"
        tools:text="Nuevo"
        android:textStyle="bold"
        android:layout_below="@+id/nuevo"
        android:layout_alignLeft="@+id/nuevo"
        android:layout_alignStart="@+id/nuevo" />

    <TextView
        android:id="@+id/textretiro"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:textAlignment="center"
        android:text="Retiro"
        android:visibility="visible"
        android:textStyle="bold"
        tools:text="Retiro"
        android:layout_below="@+id/retiro"
        android:layout_alignLeft="@+id/retiro"
        android:layout_alignStart="@+id/retiro" />

    <TextView
        android:id="@+id/textestadistica"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:text="Estadisticas"
        android:textAlignment="center"
        android:textStyle="bold"
        android:visibility="visible"
        tools:text="Estadisticas"
        android:layout_below="@+id/estadisticas"
        android:layout_alignLeft="@+id/estadisticas"
        android:layout_alignStart="@+id/estadisticas" />

    <TextView
        android:id="@+id/textconsulta"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:layout_alignLeft="@+id/consulta"
        android:layout_alignStart="@+id/consulta"
        android:layout_below="@+id/consulta"
        android:text="Consulta"
        android:textStyle="bold"
        tools:text="Consulta" />


</RelativeLayout>